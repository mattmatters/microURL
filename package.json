{
  "name": "micro-url",
  "version": "1.0.0",
  "private": true,
  "author": "Matt Lewis <domattthings@gmail.com>",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.25.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "mocha": "^3.2.0",
    "object-assign": "4.1.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.3.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "cors": "^2.8.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.14.0",
    "mongodb": "^2.2.23",
    "react-dom": "^15.6.2",
    "unix-timestamp": "^0.2.0"
  },
  "scripts": {
    "start": "npm run build && node lib/app.js",
    "build": "npm run lint && babel src -d lib",
    "lint": "eslint src/*.js",
    "test": "mocha --require ./test/testSetup.js --compilers babel-core/register ./test/*test.js",
    "test:watch": "npm test -- -w"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List",
            "Router"
          ]
        }
      ]
    }
  }
}
